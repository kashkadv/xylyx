  <script src="https://cdn.jsdelivr.net/npm/swiper@11/swiper-bundle.min.js"></script>

  <script>
    document.addEventListener('DOMContentLoaded', function () {

      function showVariant() {
        const variantEl = document.querySelector('variant-radios[data-section="{{ section.id }}"] input:checked');
        const variants = document.querySelectorAll('[data-variant-id]');
        const firstVariant = document.querySelector('[data-variant-info]');
        const prodId = window.location.search.split('=')[1];
        const variantInputControl = document.querySelector('.variant__controls input[name="id"]');

        const regex = /template--(\d+)__/;
        const inputId = variantEl.id;
        const match = inputId.match(regex);

        variants.forEach(function (variant) {
          if (prodId == variant.dataset.variantId || match[1] == variant.dataset.variantId) {
            variant.style.display = 'block';
            variant.classList.add('active');
            if (variantInputControl) {
              variantInputControl.value = prodId ? prodId : firstVariant.dataset.variantInfo;
            }
          } else if (!prodId && variant.dataset.variantId == firstVariant.dataset.variantInfo) {
            variant.style.display = 'block';
            variant.classList.add('active');
            if (variantInputControl) {
              variantInputControl.value = firstVariant.dataset.variantInfo;
            }
          } else {
            variant.style.display = 'none';
            variant.classList.remove('active');
          }
        });
      }

      showVariant();

      function sliderInit() {
          const galleries = document.querySelectorAll('.product-gallery');

          galleries.forEach((gallery, i) => {
              const sliderFull = gallery.querySelector('.product-gallery-full');
              const sliderThumb = gallery.querySelector('.product-gallery-thumb');

              sliderFull.classList.add('slider-product-' + i);
              sliderThumb.classList.add('slider-thumb-' + i);

              const swiper = new Swiper('.slider-thumb-' + i, {
                  spaceBetween: 10,
                  slidesPerView: 5,
                  freeMode: true,
                  watchSlidesProgress: true,
              });

              const swiper2 = new Swiper('.slider-product-' + i, {
                  thumbs: {
                      swiper: swiper,
                  },
              });

          });
      }

      sliderInit();

      function initGallerySlider(gallery) {
          const sliderFull = gallery.querySelector('.product-gallery-modal__item-slider');
          const sliderThumb = gallery.querySelector('.product-gallery-modal__item-thumb');

          const swiperThumb = new Swiper(sliderThumb, {
              spaceBetween: 10,
              slidesPerView: 5,
            },
          );

          const swiperSlider = new Swiper(sliderFull, {
              effect : 'fade',
              fadeEffect: {
                      crossFade: true
              },
              navigation: {
                  nextEl: ".product-gallery-modal__nav-next",
                  prevEl: ".product-gallery-modal__nav-prev",
              },
              thumbs: {
                  swiper: swiperThumb,
              },
          });

          return {
            swiperSlider,
            swiperThumb
          };
      }

      function openGalleryModal() {
          const activeSlider = document.querySelectorAll('.product-gallery-full__img');
          const activeThumb = document.querySelectorAll('.product-gallery-thumb .image');
          const body = document.querySelector('body');
          const html = document.querySelector('html');
          const main = document.querySelector('main');

          activeSlider.forEach((item) => {
              item.addEventListener('click', function () {
                  const variantId = item.closest('[data-variant-id]').dataset.variantId;
                  const itemId = item.id;
                  const ariaLabel = item.getAttribute('aria-label').split('/');
                  const itemPos = ariaLabel[0] - 1;

                  const modalEl = document.querySelector(`.product-gallery-modal[data-modal-id="${variantId}"]`);
                  const sliders = initGallerySlider(modalEl);

                  sliders.swiperSlider.on('slideChange', function () {
                    const activeSlide = sliders.swiperSlider.realIndex;

                    if (activeSlide === sliders.swiperSlider.slides.length - 1) {
                      modalEl.querySelector('.product-gallery-modal__item-thumb').classList.add('fade-first');
                      modalEl.querySelector('.product-gallery-modal__item-thumb').classList.remove('fade-last');
                    } else {
                      modalEl.querySelector('.product-gallery-modal__item-thumb').classList.add('fade-last');
                      modalEl.querySelector('.product-gallery-modal__item-thumb').classList.remove('fade-first');
                    }
                  });

                  modalEl.classList.add('active');

                  setTimeout(() => {
                    body.style.overflow = 'hidden';
                    html.style.overflow = 'hidden';
                    main.style.zIndex = 'unset';
                    modalEl.style.transform = 'translateY(0)';
                    sliders.swiperSlider.slideTo(itemPos);
                    sliders.swiperSlider.updateProgress()
                  }, 100);

                  const modalThumbs = document.querySelectorAll('.product-gallery-modal.active .product-gallery-modal__item-thumb .swiper-slide__thumb');
                  const thumbsNum = modalThumbs.length;

                  modalThumbs.forEach((item, index) => {
                    item.addEventListener('click', function() {
                      const ariaLabelThumb = parseInt(item.getAttribute('aria-label').split('/'));
                      if (ariaLabelThumb == thumbsNum) {
                        modalEl.querySelector('.product-gallery-modal__item-thumb').classList.add('fade-first');
                        modalEl.querySelector('.product-gallery-modal__item-thumb').classList.remove('fade-last');
                      } else {
                        modalEl.querySelector('.product-gallery-modal__item-thumb').classList.add('fade-last');
                        modalEl.querySelector('.product-gallery-modal__item-thumb').classList.remove('fade-first');
                      }
                    })
                  })
              });
          });

          activeThumb.forEach((item) => {
            item.addEventListener('click', function () {
                  const variantId = item.closest('[data-variant-id]').dataset.variantId;
                  const itemId = item.id;
                  const ariaLabel = item.getAttribute('aria-label').split('/');
                  const itemPos = ariaLabel[0] - 1;

                  const modalEl = document.querySelector(`.product-gallery-modal[data-modal-id="${variantId}"]`);
                  const sliders = initGallerySlider(modalEl);

                  sliders.swiperSlider.on('slideChange', function () {
                    const activeSlide = sliders.swiperSlider.realIndex;

                    if (activeSlide === sliders.swiperSlider.slides.length - 1) {
                      modalEl.querySelector('.product-gallery-modal__item-thumb').classList.add('fade-first');
                      modalEl.querySelector('.product-gallery-modal__item-thumb').classList.remove('fade-last');
                    } else {
                      modalEl.querySelector('.product-gallery-modal__item-thumb').classList.add('fade-last');
                      modalEl.querySelector('.product-gallery-modal__item-thumb').classList.remove('fade-first');
                    }
                  });

                  modalEl.classList.add('active');

                  setTimeout(() => {
                    body.style.overflow = 'hidden';
                    html.style.overflow = 'hidden';
                    main.style.zIndex = 'unset';
                    modalEl.style.transform = 'translateY(0)';
                    sliders.swiperSlider.slideTo(itemPos);
                    sliders.swiperSlider.updateProgress()
                  }, 100);

                  const modalThumbs = document.querySelectorAll('.product-gallery-modal.active .product-gallery-modal__item-thumb .swiper-slide__thumb');
                  const thumbsNum = modalThumbs.length;

                  modalThumbs.forEach((item, index) => {
                    item.addEventListener('click', function() {
                      const ariaLabelThumb = parseInt(item.getAttribute('aria-label').split('/'));
                      if (ariaLabelThumb == thumbsNum) {
                        modalEl.querySelector('.product-gallery-modal__item-thumb').classList.add('fade-first');
                        modalEl.querySelector('.product-gallery-modal__item-thumb').classList.remove('fade-last');
                      } else {
                        modalEl.querySelector('.product-gallery-modal__item-thumb').classList.add('fade-last');
                        modalEl.querySelector('.product-gallery-modal__item-thumb').classList.remove('fade-first');
                      }
                    })
                  })
              });
          });
      }

      function closeGalleryModal() {
          const modalClose = document.querySelectorAll('.product-gallery-modal__header .close');
          const body = document.querySelector('body');
          const html = document.querySelector('html');
          const main = document.querySelector('main');

          modalClose.forEach((item) => {
              item.addEventListener('click', function () {
                  const modalEl = item.closest('.product-gallery-modal');
                  modalEl.classList.remove('active');
                  body.style.overflow = '';
                  html.style.overflow = '';
                  main.style.zIndex = '';
                  modalEl.style.transform = 'translateY(-100%)';
              });
          });
      }

      function modalSliderInit() {
          const galleriesModal = document.querySelectorAll('.product-gallery-modal');
          galleriesModal.forEach((gallery) => {
              initGallerySlider(gallery);
          });
      }

      modalSliderInit();
      openGalleryModal();
      closeGalleryModal();

      function isIE() {
        const ua = window.navigator.userAgent;
        const msie = ua.indexOf('MSIE ');
        const trident = ua.indexOf('Trident/');

        return msie > 0 || trident > 0;
      }

      if (!isIE()) return;
      const hiddenInput = document.querySelector('#{{ product_form_id }} input[name="id"]');
      const noScriptInputWrapper = document.createElement('div');
      const variantSwitcher =
        document.querySelector('variant-radios[data-section="{{ section.id }}"]') ||
        document.querySelector('variant-selects[data-section="{{ section.id }}"]');
      noScriptInputWrapper.innerHTML = document.querySelector(
        '.product-form__noscript-wrapper-{{ section.id }}'
      ).textContent;
      variantSwitcher.outerHTML = noScriptInputWrapper.outerHTML;

      document.querySelector('#Variants-{{ section.id }}').addEventListener('change', function (event) {
        hiddenInput.value = event.currentTarget.value;
      });
    });

    function scrollToTop() {
      const scrollBtn = document.querySelector('#scroll-top');


      window.addEventListener('scroll', function() {
        if (window.pageYOffset > 300) {
          scrollBtn.classList.add('show');
        } else {
          scrollBtn.classList.remove('show');
        }

        const mainTag = document.querySelector('main');
        const mainTagBottom = mainTag.offsetTop + mainTag.offsetHeight;

        if (window.pageYOffset + window.innerHeight >= mainTagBottom) {
          scrollBtn.classList.add('bottom');
        } else {
          scrollBtn.classList.remove('bottom');
        }
      })

      scrollBtn.addEventListener('click', function () {
        window.scrollTo({
          top: 0,
          behavior: 'smooth'
        });
      });
    }

    scrollToTop();

    // function requestQuote() {
    //   const btn = document.querySelector('.quote-request');

    //   if (!btn) return;

    //   btn.addEventListener('click', function (e) {
    //     e.preventDefault();
    //     const form = document.querySelector('.get-in-touch');
    //     const formTop = form.offsetTop;
    //     window.scrollTo({
    //       top: formTop,
    //       behavior: 'smooth'
    //     });
    //   });
    // }

    // requestQuote();


    const wrappWords = ( blockWords ) => {
      let content = '';

      blockWords.forEach( (word, index) => {
        content += `<span class="word"><span class="word__animation" style="transition-delay:${(index + 1) * 50}ms;">${word}</span></span> `;
      });

      return content;
    }

    const animationTitle = (content) => {
      const animationTitleBlocks = document.querySelectorAll(content);
      if(!animationTitleBlocks) return;

      animationTitleBlocks.forEach(block => {
        const blockWords = block.textContent.split(" ");
        const blockTextContent = wrappWords(blockWords);
        block.textContent = '';
        block.insertAdjacentHTML('beforeend', blockTextContent);
      });
    }

    animationTitle('.animation-title__title');


  </script>



